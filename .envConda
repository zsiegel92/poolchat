venv=condaEnv

currentvenv=""
#CONDA_PREFIX=/Users/Zach/condaPool/condaEnv
#VIRTUAL_ENV=/Users/Zach/poolchat/env <- from a dif project using this script
if [[ $CONDA_PREFIX != "" ]]
then
  # Strip out the path and just leave the env name
  currentvenv="${CONDA_PREFIX##*/}"
fi

if [[ "$currentvenv" != "$venv" ]]
then
  echo "Switching to environment: $venv"
  source activate $venv
else
  echo Still in Conda environment $venv
fi

activate() {
  source activate $venv
}
deactivate() {
  source deactivate $venv
}

cpip() {
    PIP_REQUIRE_VIRTUALENV="" pip "$@"
}

